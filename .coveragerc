# .coveragerc file to configure coverage report

[run]
branch = True
source = nngt/

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    raise AssertionError
    raise NotImplementedError
    raise AttributeError
    raise InvalidArgument
    if __name__ == .__main__.:

include = nngt/*

omit =
    **/setup.py
    nngt/database/*
    nngt/testing/*
    nngt/simulation/*
    nngt/lib/db_tools.py
